<h1>Listing parts</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Quantity</th>
    <th>Link</th>
    <th>Cost</th>
    <th>Quad</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @parts.each do |part| %>
  <tr>
    <td><%= part.name %></td>
    <td><%= part.quantity %></td>
    <td><%= part.link %></td>
    <td><%= part.cost %></td>
    <td><%= part.project_id %></td>
    <td><%= link_to 'Show', part %></td>
    <td><%= link_to 'Edit', edit_part_path(part) %></td>
    <td><%= link_to 'Destroy', part, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Part', new_part_path %>
